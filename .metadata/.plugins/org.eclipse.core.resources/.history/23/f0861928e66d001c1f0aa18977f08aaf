		this.id=id;
		this.balance=balance;
	}
	
	
	public double getMonthlyInterestRate() {
		return annualInterestRate;
	}
	
	public void withdraw(int amount) {
		
	}
	
	public void deposit(int amount) {
		
	}

//	public static void main(String[] args) {
